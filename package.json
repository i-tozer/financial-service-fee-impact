{
  "name": "fee-map-2",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc && node dist/index.js",
    "ts-dev": "ts-node index.ts",
    "react-dev": "parcel src/index.html",
    "react-dev:stable": "PARCEL_WORKERS=1 parcel src/index.html --no-hmr --no-cache --port 3000 --no-content-hash",
    "react-build-once": "PARCEL_WORKERS=1 parcel build src/index.html --dist-dir ./dist-web --no-cache --no-source-maps",
    "react-serve": "npx http-server ./dist-web -p 3000",
    "react-build": "parcel build src/index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist .parcel-cache dist-web"
  },
  "dependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "cli-table3": "^0.6.5",
    "decimal.js": "^10.5.0",
    "papaparse": "^5.5.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.3",
    "@types/decimal.js": "^7.4.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.14.0",
    "@types/papaparse": "^5.3.15",
    "autoprefixer": "^10.4.21",
    "jest": "^29.7.0",
    "parcel": "^2.14.4",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.3",
    "ts-jest": "^29.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
